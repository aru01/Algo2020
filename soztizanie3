#include <iostream>
using namespace std;
int main(){
  int n;
  cin >> n;
  bool a = false;
    
    int mas[1000][1000];
    
     for(int i = 0 ; i < n ; i++)
      {
          for(int j = 0; j < n; j++)
          {
              cin >> mas[i][j];
          }
      }
    
    for(int i = 0 ; i < n ; i++)
    {
        for(int j = 0; j < n; j++)
        {
            if(mas[i][j] != mas[j][i]) {cout << "no";
            return 0;
            }
            else  a = false;
           
            }
    }
    if( a == false) cout <<"yes";
     
return 0;
}
    }